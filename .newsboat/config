browser		"qutebrowser %u"
player		"setsid -f mpvc -a '%u' > /dev/null"

auto-reload	yes
delete-played-files yes
download-path "~/rxv/pod/%h/%n"

bind-key 	G	end
bind-key	g	home
bind-key	j	down
bind-key	k	up

macro m set browser "setsid -f mpvc -a '%u' > /dev/null" ; open-in-browser ; set browser "qutebrowser %u"
